{
  "name": "@stringke/prisma-generator-tsrpc",
  "description": "tsrpc support",
  "version": "1.0.5",
  "main": "dist/generator.js",
  "license": "MIT",
  "bin": {
    "prisma-generator-tsrpc": "dist/bin.js"
  },
  "engines": {
    "node": ">=14.0"
  },
  "scripts": {
    "start": "node dist/bin.js",
    "dev": "npx tsc -w",
    "build": "rm -rf dist && npx tsc",
    "prepack": "yarn build",
    "test": "jest",
    "prepare": "husky install"
  },
  "dependencies": {
    "@prisma/generator-helper": "^4.6.1",
    "@prisma/internals": "^4.6.1",
    "prettier": "^2.7.0"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-conventional": "17.2.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^29.2.3",
    "@types/prettier": "^2.7.1",
    "husky": "8.0.2",
    "jest": "^29.3.1",
    "semantic-release": "^19.0.5",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3"
  },
  "homepage": "https://github.com/StringKe/prisma-generator-tsrpc",
  "repository": {
    "type": "git",
    "url": "https://github.com/StringKe/prisma-generator-tsrpc.git"
  },
  "author": "StringKe <authemail@qq.com>",
  "keywords": [
    "prisma",
    "prisma2",
    "generator"
  ],
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): set `package.json` to ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
